#include <stdio.h>

int main() {
    int num[5]; 
    int sum = 0, i;
    printf("enter 5 numbers: ");
    scanf("%d %d %d %d %d",&num[0],&num[1],&num[2],&num[3],&num[4]);
    for(i=0; i<=4; i++){
        sum= sum+num[i];
    }
    printf("Sum is = %d\n", sum);
    printf("avg is = %.2f", (float)sum/5);
    return 0;
}
