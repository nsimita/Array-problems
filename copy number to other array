#include <stdio.h>

int main()
{
      int i, n, a1[100],a2[100];
      printf("how many number ");
      scanf("%d",&n);
      for(i=0; i<=n-1; i++){
          scanf("%d",&a1[i]);
      }
      printf("Array 1:  ");
      for(i=0; i<=n-1; i++){
        printf("%d ", a1[i]);
      }
       for(i=0; i<=n-1; i++){
        a2[i]=a1[i];
      }
       printf("\nArray 2:  ");
       for(i=0; i<=n-1; i++){
        printf("%d ", a2[i]);
      }
    return 0;
}
